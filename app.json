{
  "expo": {
    "name": "Happy Arz",
    "slug": "happy-arz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "happyarz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font", 
      "expo-web-browser",
      "expo-camera",
      "expo-location",
      [
        "expo-router",
        {
          "origin": "https://happyarz.app/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}